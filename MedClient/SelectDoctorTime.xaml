<Window x:Name="SelectDoctorTime1" x:Class="MedClient.SelectDoctorTime"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SelectDoctorTime" Height="148" Width="293" Loaded="SelectDoctorTime1_Loaded">
    <Grid Background="#FFE5E5E5" >
        <StackPanel Margin="50,22,63,50"  Orientation="Horizontal">
            <ListBox Name="hours"  Width="73" Height="28"/>
            <Label DataContext=":" Margin="8,0,0,0"></Label>
            <ListBox Name="minutes"  Width="73" Height="28" Margin="5,0,0,0"/>
        </StackPanel>
        <Button Name="selectDoctorButton" HorizontalAlignment="Center"  Width="178" Height="28" Content="Select" Margin="50,79,57,10" Click="selectDoctorButton_Click"></Button>


    </Grid>
</Window>
